<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="Amazon Automation" parallel="false">


    <parameter name="url" value="https://www.walmart.com"/>

    <test name = "Amazon Home">
        <classes>
            <class name="homeWalmart.TestSearchOption"/>
            <class name="homeWalmart.TestDropDownButton"/>
            <class name="homeWalmart.TestMenuIcon"/>
            <class name="homeWalmart.TestStartShoppingIcon"/>
            <class name="homeWalmart.TestCartIcon"/>

        </classes>
    </test>

</suite>
