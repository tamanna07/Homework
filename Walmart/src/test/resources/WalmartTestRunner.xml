<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="Amazon Automation" parallel="false">


    <parameter name="url" value="https://www.walmart.com"/>

    <test name = "Amazon Home">
        <classes>
        <class name="homeWalmart.TestAccountIcon"></class>
        <class name="homeWalmart.TestCart"></class>
        <class name="homeWalmart.TestLOGO"></class>
        <class name="homeWalmart.TestSearchBox"></class>
        <class name="homeWalmart.TestVerticalMenu"></class>
        </classes>
    </test>

</suite>
